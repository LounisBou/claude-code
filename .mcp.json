{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  },
  "_disabled": {
    "notion": {
      "type": "http",
      "url": "https://mcp.notion.com/mcp"
    },
    "laravel-boost": {
      "type": "stdio",
      "command": "php",
      "args": ["artisan", "boost:mcp"]
    }
  }
}

{
  "additionalDirectories": ["/tmp/claude-work"],
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "autoUpdatesChannel": "stable",
  "env": {},
  "extraKnownMarketplaces": {
    "claude-plugin-directory": {
      "source": {
        "source": "github",
        "repo": "anthropics/claude-plugins-official"
      }
    },
    "superpowers-marketplace": {
      "source": {
        "source": "github",
        "repo": "obra/superpowers-marketplace"
      }
    }
  },
  "enabledPlugins": {
    "code-review@claude-plugin-directory": true,
    "commit-commands@claude-plugin-directory": true,
    "feature-dev@claude-plugin-directory": true,
    "explanatory-output-style@claude-plugin-directory": true,
    "agent-sdk-dev@claude-plugin-directory": true,
    "frontend-design@claude-plugin-directory": true,
    "plugin-dev@claude-plugin-directory": true,
    "pr-review-toolkit@claude-plugin-directory": true,
    "security-guidance@claude-plugin-directory": true,
    "alph-wiggum@claude-plugin-directory": true,
    "hookify@claude-plugin-directory": true,
    "superpowers@superpowers-marketplace": true
  },
  "hooks": {
    "PreToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/bash_logger.py"
          },
          {
            "type": "command",
            "command": ".claude/hooks/agent_logger.py"
          },
          {
            "type": "command",
            "command": ".claude/hooks/skill_logger.py"
          }
        ]
      }
    ]
  },
  "language": "english",
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true,
    "allowUnsandboxedCommands": false,
    "network": {
      "allowLocalBinding": true
